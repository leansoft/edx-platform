<div class="page-header-search wrapper-search-teams">
    <form class="teams-search-form">
        <label for="search-teams" class="search-label">><%- searchLabel %></label>
        <input id="search-teams" class="search-field" value="<%- searchString %>" placeholder="<%- searchLabel %>" />
        <button type="submit" class="action action-search"><span class="icon fa-search" aria-hidden="true"></span><span class="sr"><%- gettext('Search') %></span></button>
    </form>
</div>
